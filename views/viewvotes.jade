doctype html
head
    title= title
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/css/bootstrap.min.css')
    link(rel="stylesheet", type="text/css", href="css/rozinah.css")
body
    include header.jade
    div.container-fluid
        h1
          = title
        div.panel-heading
            table.table
                thead
                    tr
                        th Voter
                        th Rankings
                    tbody
                        each vote in votes
                            tr
                                td
                                    = vote.voter
                                td
                                    ol
                                        each val in vote.rankings
                                            li= val
        // TODO(hnag): This looks terrible; Make it look better
        p Share with others so they can vote
          &nbsp;
          a(href='/rank/' + rankid) at this link
        pre
          a(href=)
          = host
          /rank/
          = rankid

        p See results of the poll
            &nbsp;
            a(href='/viewvotes/' + voteid) at this link
        pre
          a(href=)
          = host
          /viewvotes/
          = voteid


    include footer.jade
    script(src='https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js')
    script(src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.4/js/bootstrap.min.js')
